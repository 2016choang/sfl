{
    "agent": {
        "eps_init": 0.1,
        "eps_final": 0.1,
        "eps_eval": 0.1,
        "model_kwargs": {
            "feature_size": 64,
            "dsr_params": {
                "fc_sizes": [512],
                "nonlinearity": "relu"
            }   
        },
        "idf_model_kwargs": {
            "feature_size": 64
        },
        "landmark_mode_interval": 100,
        "landmark_update_interval": 1000,
        "add_threshold": 0.75,
        "reach_threshold": 0.99,
        "max_landmarks": 10,
        "steps_per_landmark": 15,
        "true_distance": true,
        "steps_for_true_reach": 0,
        "use_oracle_landmarks": true,
        "use_soft_q": false
    },
    "algo": {
        "discount": 0.99,
        "batch_size": 128,
        "min_steps_learn": 1000,
        "learning_rate": 1e-3,
        "replay_size": 20000,
        "delta_clip": 1,
        "min_steps_dsr_learn": 10000,
        "idf_learning_rate": 5e-4
    },
    "env": {
        "size": [25, 25],
        "max_steps": 500,
        "reset_episodes": 1,
        "reset_same": false
    },
    "eval_env": {
        "size": [25, 25],
        "max_steps": 500,
        "start_pos": [7, 4],
        "reset_same": true,
        "terminate": true
    }, 
    "runner": {
        "log_interval_steps": 1e4,
        "log_dsr_interval_steps": 5e3,
        "log_landmark_steps": 5e3,
        "min_steps_landmark": 2e4
    },
    "mode": "multiroom",
    "seed": 333,
    "tabular": false
}
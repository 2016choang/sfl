{
    "agent": {
        "eps_init": 0.1,
        "eps_final": 0.1,
        "eps_eval": 0.1,
        "model_kwargs": {
            "feature_size": 64,
            "dsr_params": {
                "fc_sizes": [512],
                "nonlinearity": "relu"
            }   
        },
        "feature_model_kwargs": {
            "feature_size": 64,
            "norm_output": true,
            "alpha": 10.0
        },
        "landmark_update_interval": 1000,
        "use_oracle_graph": false,
        "use_soft_q": false
    },
    "landmarks": {
        "max_landmarks": 20,
        "add_threshold": 0.70,
        "landmark_mode_interval": 100,
        "steps_per_landmark": 15,
        "success_threshold": 0.05,
        "sim_threshold": 1,
        "use_oracle_start": false,
        "landmark_paths": 5,
        "reach_threshold": 0.99,
        "affinity_decay": 0.9
    },
    "algo": {
        "discount": 0.99,
        "batch_size": 256,
        "min_steps_learn": 1000,
        "learning_rate": 1e-3,
        "replay_size": 20000,
        "delta_clip": 1,
        "min_steps_dsr_learn": 2000,
        "feature_learning_rate": 1e-3,
        "pos_threshold": 3,
        "neg_close_threshold": 15,
        "neg_far_threshold": 30,
        "margin": 2.0
    },
    "env": {
        "size": [25, 25],
        "max_steps": 500,
        "reset_episodes": 1,
        "reset_same": false,
        "terminate": true,
        "encoding": "obj"
    },
    "eval_env": {
        "size": [25, 25],
        "max_steps": 500,
        "start_pos": [7, 4],
        "reset_same": true,
        "terminate": true,
        "encoding": "obj"
    }, 
    "runner": {
        "log_interval_steps": 1e3,
        "log_dsr_interval_steps": 1e3,
        "log_landmark_steps": 1e3,
        "min_steps_landmark_mode": 3e3
    },
    "mode": "multiroom",
    "seed": 333,
    "tabular": false,
    "feature": "TCF"
}
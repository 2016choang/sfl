{
    "agent": {
        "eps_final": 1,
        "eps_eval": 1,
        "model_kwargs": {
            "feature_size": 64,
            "dsr_params": {
                "fc_sizes": [512],
                "nonlinearity": "relu"
            }   
        },
        "idf_model_kwargs": {

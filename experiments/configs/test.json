    "env": {
        "size": [25, 25],
        "epsilon": 0.01,
        "oracle": true, 
        "grayscale": false,
        "reset_episodes": 5,
        "reset_same": false
    },
    "runner": {
        "log_interval_steps": 1e3,
        "log_dsr_interval_steps": 1e3
    },
    "mode": "multiroom",
    "seed": 333,
    "tabular": false
